{
  "name": "imdhemy/testing-es-in-php",
  "description": "How to test elasticsearch in php",
  "license": "MIT",
  "authors": [
    {
      "name": "imdhemy",
      "email": "imdhemy@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "elasticsearch/elasticsearch": "^7.14"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "ACME\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ACME\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit"
  }
}
